{
  "name": "biskott",
  "version": "0.1.0",
  "description": "Pre-configured boilerplate for Electron + React + TypeScript",
  "main": "./build/index.js",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore . --ext .js,.jsx,.ts,.tsx",
    "dev": "rimraf build && cross-env NODE_ENV=development webpack --watch --progress --colors",
    "build": "rimraf build && cross-env NODE_ENV=production webpack --progress --colors",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register \"test/**/*.ts\"",
    "package": "node ./scripts/build.js",
    "start": "npm run build && electron --no-sandbox ./build/index.js",
    "build:win": "rimraf out && electron-builder build --win",
    "build:linux": "rimraf out && electron-builder build --linux",
    "build:mac": "electron-builder build --mac --x64"
  },
  "build": {
    "appId": "com.biskott.ui",
    "productName": "Biskott UI",
    "directories": {
      "app": "./build/",
      "output": "./out/"
    },
    "mac": {
      "target": "pkg"
    },
    "win": {
      "target": {
        "target": "nsis",
        "arch": [
          "ia32"
        ]
      }
    },
    "buildVersion": "0.1.0"
  },
  "author": "Elias Platek",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@material-ui/core": "^4.9.9",
    "@material-ui/icons": "^4.9.1",
    "@sucrase/webpack-loader": "^2.0.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/node-fetch": "^2.5.5",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "@types/webtorrent": "^0.107.2",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "acorn": "^7.1.1",
    "ajv": "^6.10.2",
    "buble": "^0.20.0",
    "buble-loader": "^0.5.1",
    "copy-pkg-json-webpack-plugin": "0.0.38",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "cross-zip": "^3.0.0",
    "electron": "^8.5.2",
    "electron-builder": "^22.4.1",
    "electron-packager": "^14.2.1",
    "electron-winstaller": "^4.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "file-loader": "^6.0.0",
    "languagedetect": "^2.0.0",
    "minimist": "^1.2.5",
    "mkdirp": "^1.0.4",
    "mocha": "^7.1.1",
    "network-address": "^1.1.2",
    "node-fetch": "^2.6.0",
    "opensubtitles-api": "^5.1.0",
    "prettier": "^2.0.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-intersection-observer-hook": "^1.0.4",
    "rimraf": "^3.0.2",
    "run-series": "^1.1.8",
    "simple-concat": "^1.0.0",
    "source-map-loader": "^0.2.4",
    "spectron": "^10.0.1",
    "srt-to-vtt": "^1.1.3",
    "sucrase": "^3.13.0",
    "ts-node": "^8.8.2",
    "typescript": "^3.7.2",
    "vlc-command": "^1.2.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.10",
    "webtorrent": "^0.108.1"
  },
  "keywords": [
    "boilerplate",
    "Electron",
    "React",
    "Typescript",
    "Webpack"
  ]
}
