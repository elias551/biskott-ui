{
  "name": "biskott",
  "version": "0.1.0",
  "description": "Pre-configured boilerplate for Electron + React + TypeScript",
  "main": "./dist/main.bundle.js",
  "scripts": {
    "start": "electron ./dist/main.bundle.js",
    "lint": "eslint --ignore-path .gitignore . --ext .js,.jsx,.ts,.tsx",
    "dev": "rimraf dist && cross-env NODE_ENV=development webpack --watch --progress --colors",
    "prod": "rimraf dist && cross-env NODE_ENV=production webpack --progress --colors",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register \"test/**/*.ts\"",
    "build:win": "rimraf out && electron-builder build --win",
    "build:mac": "electron-builder build --mac --x64",
    "build:linux": "electron-builder build --linux"
  },
  "build": {
    "appId": "com.biskott.ui",
    "productName": "Biskott UI",
    "directories": {
      "app": "./dist/",
      "output": "./out/"
    },
    "mac": {
      "target": "pkg"
    },
    "win": {
      "target": {
        "target": "nsis",
        "arch": [
          "ia32"
        ]
      }
    },
    "buildVersion": "0.1.0"
  },
  "author": "Elias Platek",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@material-ui/core": "^4.9.9",
    "@material-ui/icons": "^4.9.1",
    "languagedetect": "^2.0.0",
    "network-address": "^1.1.2",
    "node-fetch": "^2.6.0",
    "opensubtitles-api": "^5.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "simple-concat": "^1.0.0",
    "srt-to-vtt": "^1.1.3",
    "vlc-command": "^1.2.0",
    "webtorrent": "^0.108.1",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/node-fetch": "^2.5.5",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "@types/webtorrent": "^0.107.2",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "acorn": "^7.1.1",
    "ajv": "^6.10.2",
    "babel-loader": "^8.1.0",
    "copy-pkg-json-webpack-plugin": "0.0.38",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "electron": "^8.2.0",
    "electron-builder": "^22.4.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "file-loader": "^6.0.0",
    "mocha": "^7.1.1",
    "prettier": "^2.0.2",
    "rimraf": "^3.0.2",
    "source-map-loader": "^0.2.4",
    "spectron": "^10.0.1",
    "ts-node": "^8.8.2",
    "typescript": "^3.7.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.10"
  },
  "keywords": [
    "boilerplate",
    "Electron",
    "React",
    "Typescript",
    "Webpack"
  ]
}
